{
  "name": "@frontendmonster/utils",
  "version": "0.2.1",
  "license": "MIT",
  "author": "frontendmonster <frontendmonster@gmail.com>",
  "description": "Common JavaScript utility functions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendmonster/utils.git"
  },
  "main": "./bundles/index.umd.js",
  "module": "./esm5/index.js",
  "es2015": "./esm2015/index.js",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "commit": "git-cz",
    "clean": "shx rm -rf lib/*",
    "prebuild": "yarn clean && yarn verify",
    "build": "tsc && tsc --target es2018 --outDir lib/esm2015 && rollup -c && rollup -c --environment NODE_ENV:production",
    "lint": "eslint src --ext js,ts",
    "lint:fix": "yarn lint --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "lib:build": "builder",
    "lib:pack": "builder --pack",
    "prerelease": "builder && standard-version",
    "release": "yarn release:github && yarn release:npm",
    "release:github": "git push --no-verify --follow-tags origin master",
    "release:npm": "npm publish lib",
    "release:preflight": "npm pack ./lib --dry-run",
    "verify": "yarn run lint && yarn run test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@frontendmonster/builder": "1.0.0",
    "@frontendmonster/eslint-config": "3.0.4",
    "@types/jest": "24.9.0",
    "@types/node": "13.1.8",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.8.0",
    "husky": "4.0.10",
    "jest": "24.9.0",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "rollup": "1.29.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.5.0",
    "rollup-plugin-uglify": "6.0.4",
    "shx": "0.3.2",
    "standard-version": "7.0.1",
    "ts-jest": "24.3.0",
    "typescript": "3.7.5",
    "webpack-config-utils": "2.3.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "yarn test --bail --onlyChanged"
    }
  }
}
