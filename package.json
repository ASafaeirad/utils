{
  "name": "@frontendmonster/utils",
  "version": "0.1.1",
  "license": "MIT",
  "author": "frontendmonster <frontendmonster@gmail.com>",
  "description": "Common JavaScript utility functions",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontendmonster/utils.git"
  },
  "main": "./bundles/index.umd.js",
  "module": "./esm5/index.js",
  "es2015": "./esm2015/index.js",
  "typings": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "commit": "git-cz",
    "clean": "shx rm -rf lib/*",
    "prebuild": "npm run clean && npm run verify",
    "build": "tsc && tsc --target es2018 --outDir lib/esm2015 && rollup -c && rollup -c --environment NODE_ENV:production",
    "lint": "tslint --project tsconfig.json --fix",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "lib:build": "builder",
    "lib:pack": "builder --pack",
    "prerelease": "standard-version",
    "release": "builder && npm run release:github && npm run release:npm",
    "release:github": "git push --no-verify --follow-tags origin master",
    "release:npm": "npm publish lib",
    "release:preflight": "npm pack ./lib --dry-run",
    "verify": "npm run lint && npm run test"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@frontendmonster/builder": "1.0.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.5",
    "commitizen": "4.0.3",
    "cz-conventional-changelog": "3.0.2",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "prettier": "1.18.2",
    "rollup": "1.21.4",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "6.0.3",
    "shx": "0.3.2",
    "standard-version": "7.0.0",
    "ts-jest": "24.1.0",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.6.3",
    "webpack-config-utils": "2.3.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm test -- --bail --onlyChanged"
    }
  }
}
